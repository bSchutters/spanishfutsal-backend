{
  "collectionName": "lffs_updates",
  "info": {
    "singularName": "lffs-update",
    "pluralName": "lffs-updates",
    "displayName": "LFFS Update",
    "description": "Infos sur les dernières mises à jour"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "type": {
      "type": "enumeration",
      "enum": ["ranking", "matches"],
      "required": true
    },
    "last_update": {
      "type": "datetime"
    },
    "status": {
      "type": "enumeration",
      "enum": ["success", "error", "in_progress"],
      "default": "success"
    },
    "error_message": {
      "type": "text"
    },
    "items_processed": {
      "type": "integer"
    }
  }
}
